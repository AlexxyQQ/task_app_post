  "version": "0.2.0",
  "configurations": [
    {
      "name": "app (Development)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--no-enable-impeller"]
    },
    {
      "name": "app (Profile Mode)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "profile",
      "args": ["--no-enable-impeller"]
    },
    {
      "name": "app (Release Mode)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "release",
      "args": ["--no-enable-impeller"]
    }
  ]
}
